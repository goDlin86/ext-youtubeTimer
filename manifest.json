{
    "manifest_version": 3,
  
    "name": "youtubeTimer",
    "description": "Youtube Timer",
    "version": "1.5",

    "action": {
        "default_title": "Show timer",
        "default_popup": "dist/popup.html"
    },

    "background": {
        "service_worker": "background.js"
    },

    "permissions": ["tabs", "storage"]
}