{
    "manifest_version": 3,
  
    "name": "youtubeTimer",
    "description": "Youtube Timer",
    "version": "1.5",

    "action": {
        "default_icon": { 
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
           "128": "icons/icon128.png"
        },
        "default_title": "Show timer",
        "default_popup": "dist/index.html"
    },

    "background": {
        "service_worker": "background.js"
    },

    "permissions": ["tabs", "storage"]
}